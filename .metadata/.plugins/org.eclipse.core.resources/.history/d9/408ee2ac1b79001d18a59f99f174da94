  public void f() 
  {
	  String a="ashok";
	  String b="ashok";
	  
	  soft.assertEquals(a,b,"TC is failed a and b are equals");
	  
	  soft.assertNotNull(b,"TC is failed b is null");
	  
	  
	  
	  
  }
  @Test
  public void x() 
  {
	  
  }
  
